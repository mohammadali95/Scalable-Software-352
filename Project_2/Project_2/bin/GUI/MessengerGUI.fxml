<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.GUIController">
   <center>
      <AnchorPane prefHeight="373.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <SplitPane fx:id="splitpane" dividerPositions="0.11320754716981132" orientation="VERTICAL" prefHeight="373.0" prefWidth="160.0">
               <items>
                  <AnchorPane fx:id="buttonPane" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <Button fx:id="addConversation" layoutX="14.0" layoutY="6.0" mnemonicParsing="false" onAction="#makeNewConversation" text="ADD" />
                        <Button fx:id="gotoButton" layoutX="79.0" layoutY="6.0" mnemonicParsing="false" onAction="#gotoConversation" text="Go To!" />
                     </children>
                  </AnchorPane>
                  <AnchorPane fx:id="listPane" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <ListView fx:id="conversationList" layoutX="4.0" layoutY="9.0" prefHeight="321.0" prefWidth="150.0" />
                     </children>
                  </AnchorPane>
               </items>
            </SplitPane>
            <Pane fx:id="conversationViewer" layoutX="172.0" layoutY="11.0" prefHeight="347.0" prefWidth="206.0" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
